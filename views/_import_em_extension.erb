<script type="text/javascript">

require(['jquery', 'YSDEntityManagementComplementHooks', 'YSDImportForm'],
        function($, EntityManagementComplementHooks, ImportForm) {

  /* ------------------------------------------------
     Plugin for import form
     ------------------------------------------------ */
  ImportFormHookPlugin = function() {
	
    this.importForm = null;

    this.onRenderEntities = function(entities) {
      this.importForm = new ImportForm();
    }

  }
	
  var importFormHookPlugin = new ImportFormHookPlugin();
  EntityManagementComplementHooks.register(importFormHookPlugin);	

});
        
</script>